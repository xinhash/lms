    "@tsed/exceptions": "^6.62.0",
    "@tsed/json-mapper": "^6.62.0",
    "@tsed/mongoose": "^6.62.0",
    "@tsed/passport": "^6.62.0",
    "@tsed/platform-express": "^6.62.0",
    "@tsed/schema": "^6.62.0",
    "@tsed/swagger": "^6.62.0",
    "ajv": "^8.6.2",
    "argon2-ffi": "^2.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
